<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="action_enable_disable_product" model="ir.actions.act_window">
        <field name="name">Enable/Disable Products</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">enable.disable.product</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="enable_disable_product_view_form"/>
        <field name="binding_model_id" ref="product.model_product_product"/>
        <field name="target">new</field>
    </record>

    <record id="action_enable_products_as_is" model="ir.actions.server">
        <field name="name">Enable for Stores</field>
        <field name="model_id" ref="product.model_product_product"/>
        <field name="binding_model_id" ref="product.model_product_product"/>
        <field name="state">code</field>
        <field name="code">
            if records:
                records.enable_all_products()
        </field>
    </record>

</odoo>
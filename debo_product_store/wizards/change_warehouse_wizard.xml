<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_product_change_warehouse" model="ir.ui.view">
            <field name="name">Change Product Warehouses</field>
            <field name="model">change.warehouse.wizard</field>
            <field name="arch" type="xml">
              <form string="Change Product Warehouses">
                  <separator string="Change Warehouses"/>
                  <group>
                          <field name="product_ids" invisible="1"/>
                          <field name="warehouse_ids" widget="many2many_tags" options="{'no_open':True,'no_create':True,'no_create_edit':True}"/>
                  </group>
                  <footer>
                      <button name="action_confirm" string="_Apply" type="object" class="btn-primary"/>
                      <button string="Cancel" class="btn-secondary" special="cancel" />
                  </footer>
              </form>
            </field>
        </record>

        <record id="action_view_product_change_warehouse" model="ir.actions.act_window">
            <field name="name">Change Product Warehouses</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">change.warehouse.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_product_change_warehouse"/>
            <field name="target">new</field>
        </record>
</odoo>

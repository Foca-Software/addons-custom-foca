<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables">
        <xpath expr="//link[@href='/web/static/src/scss/primary_variables.scss']" position="replace">
            <link rel="stylesheet" type="text/scss" href="/web_debo_theme/static/src/scss/primary_variables.scss" />
        </xpath>
    </template>

    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/web_debo_theme/static/src/scss/web_customization.scss" />
        </xpath>
    </template>
</odoo>